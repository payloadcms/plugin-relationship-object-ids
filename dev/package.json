{
  "name": "payload-plugin-relationship-object-ids-demo",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "rimraf src/uploads && nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "yarn build:payload && yarn build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "test": "rimraf src/uploads && jest --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "^1.4.3",
    "@payloadcms/plugin-nested-docs": "^1.0.12",
    "@payloadcms/richtext-lexical": "^0.7.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "payload": "^2.11.2",
    "rimraf": "^5.0.1"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.26",
    "@types/express": "^4.17.9",
    "@types/jest": "^29.5.2",
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^8.13.0",
    "nodemon": "^2.0.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.3"
  }
}
